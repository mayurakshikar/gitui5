<mvc:View
    controllerName="ns.gitui5.controller.View1"
    class="sapUiSizeCompact"
    xmlns="sap.ui.table"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns:l="sap.ui.layout"
    xmlns:f="sap.ui.layout.form"
    xmlns:u="sap.ui.unified"
    xmlns:core="sap.ui.core"
    xmlns:m="sap.m"
    displayBlock="true"
>
	<m:Shell id="shell">
		<m:App id="app">
			<m:pages>
				<m:Page id="page" title="{i18n>title}">

	            <l:Grid defaultSpan="L12 M12 S12" class="sapUiSmallMarginTop sapUiMediumMargin">
                <l:content>
                <f:SimpleForm id="SimpleForm"
                          maxContainerCols="2"
                          editable="true"
                          layout="ResponsiveGridLayout"
                          labelSpanL="3"
                          labelSpanM="5"
                          emptySpanL="0"
                          emptySpanM="0"
                          columnsL="2"
                          columnsM="2"
                          adjustLabelSpan="false"
                          class="editableForm">
                <f:content>
                    <m:Label text="{i18n>sn}" />
                    <m:Input value="{SupplierName}" >
                        <m:layoutData>
                            <l:GridData span="L3 M3 S7" />
                        </m:layoutData>
                    </m:Input>
                    <m:Label text="{i18n>str}" />
                    <m:Input value="{Street}">
                        <m:layoutData>
                            <l:GridData span="L3 M3 S7" />
                        </m:layoutData>
                    </m:Input>
                    <m:Label text="{i18n>date}" />
                    <m:DatePicker
                                id="DP4"
                                value="{path:'/dateValue', type:'sap.ui.model.type.Date', formatOptions: { style: 'medium', strictParsing: true}}"
                                class="sapUiSmallMarginBottom"
                                >
                        <m:layoutData>
                            <l:GridData span="L2 M2 S7" />
                        </m:layoutData>
                    </m:DatePicker>
                    <!-- <m:Label text="{i18n>comm}" />
                    <m:Select items="{      path: 'ScreenModel>/commerciaux',
                                            sorter: { path: 'long_name' }
                                        }"
                                        autoAdjustWidth="true">
                        <core:Item text="{ScreenModel>long_name}" key="{ScreenModel>id}" />
                    </m:Select> -->
                    <m:Label text="{i18n>store}" />
                    <m:Select width="100%">
                    <m:items>
                        <core:Item text="Germany Store 1" />
                        <core:Item text="Germany Store 2" />
                        <core:Item text="Germany Store 3" />
                        <core:Item text="USA Store 2" />
                        <core:Item text="England Store 1" />
                    </m:items>
                    <m:layoutData>
                        <l:GridData span="L3 M3 S7" />
                    </m:layoutData>
                    </m:Select>

                    <m:Label text="{i18n>clnt}" />
                    <m:Input value="{SupplierName}" liveChange="onClient" >
                    <m:layoutData>
                        <l:GridData span="L2 M2 S7" />
                    </m:layoutData>
                    </m:Input>
                    <m:Label text="Street/No." />
                <m:Input value="{Street}">
                    <m:layoutData>
                        <l:GridData span="L2 M2 S7" />
                    </m:layoutData>
                </m:Input>
                <m:Input value="{HouseNumber}">
                    <m:layoutData>
                        <l:GridData span="L2 M2 S7" />
                    </m:layoutData>
                </m:Input>
                <m:Label text="ZIP Code/City" />
                <m:Input value="{ZIPCode}">
                    <m:layoutData>
                        <l:GridData span="L2 M2 S7" />
                    </m:layoutData>
                </m:Input>
                <m:Input value="{City}" >
                <m:layoutData>
                        <l:GridData span="L2 M2 S7" />
                    </m:layoutData>
                </m:Input>
                <m:Label text="Country" />
                <m:Select width="100%">
                    <m:items>
                        <core:Item text="Germany" />
                        <core:Item text="USA" />
                        <core:Item text="England" />
                    </m:items>
                    <m:layoutData>
                        <l:GridData span="L2 M2 S7" />
                    </m:layoutData>
                </m:Select>
                <m:Label ></m:Label>
                <m:Button text="Save" type="Accept">
                <m:layoutData>
                        <l:GridData span="L2 M2 S7" />
                </m:layoutData>
                </m:Button> 
                <m:Button text="Cancel" type="Reject">
                <m:layoutData>
                        <l:GridData span="L2 M2 S7" />
                </m:layoutData>
                </m:Button> 
                </f:content>
                </f:SimpleForm>
        
                </l:content>
                </l:Grid>
				</m:Page>
			</m:pages>
		</m:App>
	</m:Shell>
</mvc:View>
